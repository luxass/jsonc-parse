{
  "name": "jsonc-parse",
  "type": "module",
  "version": "1.1.0",
  "packageManager": "pnpm@8.9.2",
  "description": "A lightweight JSON with Comments parser.",
  "author": {
    "name": "Lucas Nørgård",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "license": "MIT",
  "homepage": "https://github.com/luxass/jsonc-parse",
  "repository": {
    "type": "git",
    "url": "https://github.com/luxass/jsonc-parse"
  },
  "bugs": {
    "url": "https://github.com/luxass/jsonc-parse/issues"
  },
  "keywords": [
    "jsonc",
    "json",
    "comments",
    "jsonc-parse"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./strip": {
      "import": {
        "types": "./dist/strip.d.ts",
        "default": "./dist/strip.mjs"
      },
      "require": {
        "types": "./dist/strip.d.cts",
        "default": "./dist/strip.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest --run",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@luxass/strip-json-comments": "^1.0.0"
  },
  "devDependencies": {
    "@luxass/eslint-config": "^3.3.2",
    "@types/node": "^18.16.16",
    "eslint": "^8.52.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  }
}
