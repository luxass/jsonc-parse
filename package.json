    "test": "vitest --run",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@luxass/strip-json-comments": "2.0.0"
  },
  "devDependencies": {
    "@luxass/eslint-config": "6.0.1",
    "@types/node": "22.16.5",
    "eslint": "9.39.1",
    "publint": "0.3.15",
    "tsdown": "0.16.0",
    "typescript": "5.9.3",
    "vitest": "4.0.7"
  }
