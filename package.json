{
  "name": "jsonc-parse",
  "version": "1.5.5",
  "description": "A lightweight JSON with Comments parser.",
  "type": "module",
  "author": {
    "name": "Lucas Nørgård",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "packageManager": "pnpm@10.13.1",
  "license": "MIT",
  "homepage": "https://github.com/luxass/jsonc-parse",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luxass/jsonc-parse.git"
  },
  "bugs": {
    "url": "https://github.com/luxass/jsonc-parse/issues"
  },
  "keywords": [
    "jsonc",
    "json",
    "comments",
    "jsonc-parse"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./strip": {
      "import": {
        "types": "./dist/strip.d.ts",
        "default": "./dist/strip.mjs"
      },
      "require": {
        "types": "./dist/strip.d.cts",
        "default": "./dist/strip.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest --run",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@luxass/strip-json-comments": "1.4.0"
  },
  "devDependencies": {
    "@luxass/eslint-config": "4.19.0",
    "@types/node": "22.16.5",
    "eslint": "^9.17.0",
    "tsup": "8.5.0",
    "typescript": "5.8.3",
    "vitest": "3.1.4"
  }
}
