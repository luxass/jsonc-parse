{
  "name": "jsonc-parse",
  "version": "2.0.0",
  "description": "A lightweight JSON with Comments parser.",
  "type": "module",
  "author": {
    "name": "Lucas Nørgård",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "packageManager": "pnpm@10.27.0",
  "license": "MIT",
  "homepage": "https://github.com/luxass/jsonc-parse",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luxass/jsonc-parse.git"
  },
  "bugs": {
    "url": "https://github.com/luxass/jsonc-parse/issues"
  },
  "keywords": [
    "jsonc",
    "json",
    "comments",
    "jsonc-parse"
  ],
  "exports": {
    ".": "./dist/index.mjs",
    "./strip": "./dist/strip.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
