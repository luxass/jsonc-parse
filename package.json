{
  "name": "jsonc-parse",
  "type": "module",
  "version": "1.0.4",
  "packageManager": "pnpm@8.7.5",
  "description": "A lightweight JSON with Comments parser.",
  "author": {
    "name": "Lucas Nørgård",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "license": "MIT",
  "homepage": "https://github.com/luxass/jsonc-parse",
  "repository": {
    "type": "git",
    "url": "https://github.com/luxass/jsonc-parse"
  },
  "bugs": {
    "url": "https://github.com/luxass/jsonc-parse/issues"
  },
  "keywords": [
    "jsonc",
    "json",
    "comments",
    "jsonc-parse"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest --run",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "strip-json-comments": "^5.0.1"
  },
  "devDependencies": {
    "@luxass/eslint-config": "^3.3.1",
    "@types/node": "^18.16.16",
    "eslint": "^8.46.0",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6",
    "vitest": "^0.34.1"
  }
}
